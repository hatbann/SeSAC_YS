    <form action="/postPractice" method="post" name="postpractice">
        <label for="name">이름</label>
        <input type="text" name="name" id="name" required>
        <fieldset>
            <legend>성별</legend>
            <input type="radio" name=pgender" id="male" value="남자">
            <label for="male" required>남자</label>
            <input type="radio" name="gender" id="female" value="여자"> 
            <label for="female">여자</label>
        </fieldset>
        <fieldset>
            <legend>생년월일</legend>
            <select name="year" id="year">
                <% for(let i = 1950; i<2022; i++){ %>
                    <option value="<%=i%>"><%=i%></option>
                <%} %>
            </select>년
            <select name="month" id="month">
                <% for(let i = 1; i<13; i++){ %>
                    <option value="<%=i%>"><%=i%></option>
                <%} %>
            </select>월
            <select name="day" id="day">
                <% for(let i = 1; i<31; i++){ %>
                    <option value="<%=i%>"><%=i%></option>
                <%} %>
            </select>일
        </fieldset>
        <fieldset>
            <legend>관심사</legend>
            <input type="checkbox" name="interest" id="travel" value="여행">
            <label for="travel">여행</label>
            <input type="checkbox" name="interest" id="fashion" value="패션">
            <label for="fashion">패션</label>
            <input type="checkbox" name="interest" id="food" value="음식">
            <label for="food">음식</label>
        </fieldset>
        <fieldset>
            <legend>선택</legend>
            <label for="color">좋아하는 색상</label>
            <input type="color" name="color" id="color">
            <label for="number">좋아하는 숫자</label>
            <input type="number" name="number" id="number">
        </fieldset>
        <button type="submit">회원가입</button>
    </form>
</body>
